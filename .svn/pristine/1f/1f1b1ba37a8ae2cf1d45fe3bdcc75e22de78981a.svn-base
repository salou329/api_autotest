config:
    name: "首页总览测试"
teststeps:
    - name: 登陆
      api: api/登录.yml
      extract:
          - token: cookies.JSESSIONID
      validate:
          - eq: ["content.message", "用户登录成功"]
    - name: 首页总览
      api: api/首页总览.yml
      variables:
          JSESSIONID: $token
      validate:
          - eq: ["content.data.totalNum", 14]
