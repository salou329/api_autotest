name: 功能使用统计
base_url: ${ENV(url)}
variables:
    dateStart: dateStart
	dateEnd: dateEnd
	granularity: granularity
	business: business
	JSESSIONID: ${ENV(JSESSIONID)}
request:
    url: /admin/deviceoperate
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded;
    params:
        where: "{dateStart:$dateStart,dateEnd:$dateEnd,granularity:$granularity,business:$business}"
validate:
    - eq: ["status_code", 200]
