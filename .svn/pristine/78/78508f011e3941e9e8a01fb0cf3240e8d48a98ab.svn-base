name: 修改设备信息
base_url: ${ENV(url)}
variables:
    JSESSIONID: ${ENV(JSESSIONID)}
    id: id
    divice_number: divice_number
    provinceid: provinceid
    business_hall_id: business_hall_id
    cityid: cityid
    districtid: districtid
request:
    url: /admin/device/modify
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded;
    params:
        where: "{id:$id,divice_number:$divice_number,provinceid:$provinceid,business_hall_id:$business_hall_id,cityid:$cityid,districtid:$districtid}"
validate:
    - eq: ["status_code", 200]
