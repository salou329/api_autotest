name: 更新版本
base_url: ${ENV(url)}
variables:
    JSESSIONID: ${ENV(JSESSIONID)}
    where: '{"version_content":"gfgfg","release_time":"2020-03-19 16:48","installtime":"","update_immediately":"1","businesHallIds":"f40794f7d9a1427c8592cf4bbe7db4c1","version_code":"123","version_size":0,"version_size_byte":3031,"release_account_id":"shiwenbin"}'
request:
    url: /admin/version/addVersionRecord
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded; charset=UTF-8
    params:
        where: $where
validate:
    - eq: ["status_code", 200]
