name: 更改用户可变权限
base_url: ${ENV(url)}
variables:
    id: sids
    fids: sids
    sids: sids
	JSESSIONID: ${ENV(JSESSIONID)}
request:
    url: /admin/admin/updateMenus
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded;
    params:
        where: "{id:$id,fids:$fids,sids:$sids}"
validate:
    - eq: ["status_code", 200]
