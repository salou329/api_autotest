name: 根据市id获取区列表
base_url: ${ENV(url)}
variables:
    city_id: city_id
	JSESSIONID: ${ENV(JSESSIONID)}
request:
    url: /admin/address/china/
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded;
    params:
        city_id: $city_id
validate:
    - eq: ["status_code", 200]
