name: 获取单个设备信息
base_url: ${ENV(url)}
variables:
    id: id
	JSESSIONID: ${ENV(JSESSIONID)}
request:
    url: /admin/device/modify
    method: POST
    headers:
        Cookie: JSESSIONID=$JSESSIONID
        Content-Type: application/x-www-form-urlencoded;
    params:
        id: $id
validate:
    - eq: ["status_code", 200]
